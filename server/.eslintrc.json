{
  "extends": "google",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "spread": true,
    "restParams": true,
    "experimentalObjectRestSpread": true
  },
  "rules": {
    "strict": [0],
    "no-param-reassign": [0],
    "no-unused-vars": [2, {"argsIgnorePattern": "^_"}],
    "linebreak-style": [0],
    "comma-dangle": [0],
    "require-jsdoc": [0],
    "curly": [0],
    "no-warning-comments": [0],
    "camelcase": [0]
  }
}
